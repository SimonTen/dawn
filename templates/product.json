{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "caption": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "fbc51f2f-c5db-47a5-b567-e86a52e66399": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{% render 'product-delivery-time' %}"
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Teilen"
          }
        },
        "a9289fa8-d500-45d3-8710-973e7fc420d5": {
          "type": "shopify:\/\/apps\/product-reviews\/blocks\/star_rating\/bae150af-8da8-48b2-9867-398188115e5f",
          "settings": {
            "stars_fill_color": "#000000",
            "star_size": 15,
            "text_align": "center",
            "product": "{{product}}",
            "stars_text_empty": "Keine Bewertungen"
          }
        },
        "7a77353d-5962-4f36-9643-4654651e6d2b": {
          "type": "shopify:\/\/apps\/product-reviews\/blocks\/reviews\/bae150af-8da8-48b2-9867-398188115e5f",
          "settings": {
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "caption",
        "price",
        "variant_picker",
        "quantity_selector",
        "fbc51f2f-c5db-47a5-b567-e86a52e66399",
        "buy_buttons",
        "description",
        "share",
        "a9289fa8-d500-45d3-8710-973e7fc420d5",
        "7a77353d-5962-4f36-9643-4654651e6d2b"
      ],
      "settings": {
        "enable_sticky_info": true,
        "media_position": "left",
        "gallery_layout": "stacked",
        "media_size": "small",
        "mobile_thumbnails": "hide",
        "hide_variants": true,
        "enable_video_looping": false,
        "padding_top": 36,
        "padding_bottom": 12
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "Andere Kunden kauften auch...",
        "heading_size": "h2",
        "products_to_show": 4,
        "columns_desktop": 4,
        "color_scheme": "background-1",
        "image_ratio": "square",
        "show_secondary_image": true,
        "show_vendor": true,
        "show_rating": true,
        "columns_mobile": "2",
        "padding_top": 36,
        "padding_bottom": 0
      }
    }
  },
  "order": [
    "main",
    "product-recommendations"
  ]
}
